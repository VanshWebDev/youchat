{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://youchat-snowy.vercel.app/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://youchattt.netlify.app"
        }
      ]
    }
  ]
}
